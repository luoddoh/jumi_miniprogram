/**app.wxss**/
page{
  height: 100vh;
}
.page{
  height: 99vh;
}
.content_block{
  font-size: 32rpx !important;
  height: 1140rpx !important;
}
.form_item .cell_label{
  width: 160rpx !important;
}
.skuImage{
  width: 100%;
  height: 100%;
}
.table .table_row .cell_1{
  overflow: hidden !important;
}
.table .head .cell{
  background-color: rgb(238, 238, 238) !important;
  font-weight: bold !important;
}
.row.head,.row.head .cell{
  background-color: rgb(238, 238, 238) !important;
}
.table_row:not(.head) .cell_2{
  line-height: 40rpx !important;
  display: flex;
  align-items: center;
  justify-content: center;
}
.table_row:not(.head) .cell_3{
  line-height: 40rpx !important;
  display: flex;
  align-items: center;
  justify-content: center;
}
.code_table{
  height: 1000rpx;
  border-top: 2rpx solid gray;
}
.code_table.phone{
  height: 1140rpx;
}
.code_table .row{
  height: 110rpx !important;
}
.code_table .row .cell{
  line-height: 40rpx;
  height: 120rpx;
  text-align: center;
  overflow: hidden;
}
.code_table .row .cell_2{
  width: 80rpx;
}
.code_table .row .cell_3{
  width: 260rpx;
}
.code_table .row .cell_4{
  width: 260rpx;
}
.scan_titel{
  text-align: center;
  padding: 110rpx 0 30rpx;
}
